{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "ggedde/spry-php-kit",
    "type": "project",
    "description": "The starter kit for SpryPHP.",
    "keywords": ["spryphp", "spry-php", "framework", "php", "toolkit"],
    "license": "MIT",
    "require": {
        "php": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "SpryPhp\\": "SpryPhp/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}